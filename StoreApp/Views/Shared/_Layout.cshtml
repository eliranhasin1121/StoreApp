    <div class="shipContainer">
        <div id="openweathermap-widget-19"></div>
        <div id="openweathermap-widget-11"></div>
        <script src='//openweathermap.org/themes/openweathermap/assets/vendor/owm/js/d3.min.js'></script>
        <div id="openweathermap-widget-18"></div>
        <script>
            window.myWidgetParam ? window.myWidgetParam : window.myWidgetParam = []; window.myWidgetParam.push({ id: 18, cityid: '293397', appid: 'a5bc298bdc0aab26f6afa1f4f9041002', units: 'metric', containerid: 'openweathermap-widget-18', }); (function () { var script = document.createElement('script'); script.async = true; script.charset = "utf-8"; script.src = "//openweathermap.org/themes/openweathermap/assets/vendor/owm/js/weather-widget-generator.js"; var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(script, s); })();


            function shipmentFunction() {
                var weatherStatus = document.getElementsByClassName('widget-right__description')[0].innerHTML;
                var hour = document.getElementsByClassName('widget-right__date')[0].innerHTML.split(":")[0];
                var str1 = "few clouds";
                var str2 = "Scattered clouds";
                var str3 = "scattered clouds";
                var str4 = "broken clouds";
                if (hour > 21 || hour < 10) {
                    document.getElementById('shipStatus').innerHTML = " שרות המשלוחים שלנו סגור ויחזור לפעול ב10 בבוקר";
                }
                else if (weatherStatus == str1) {
                    document.getElementById('shipStatus').innerHTML = " שרות המשלוחים שלנו סגור עקב כמה עננים בשמיים";
                }
                else if (weatherStatus == str2) {
                    document.getElementById('shipStatus').innerHTML = " שרות המשלוחים שלנו סגור עקב עננים מפוזרים בשמיים";
                }
                else if (weatherStatus == str3) {
                    document.getElementById('shipStatus').innerHTML = " שרות המשלוחים שלנו סגור עקב עננים מפוזרים בשמיים";
                }
                else if (weatherStatus == str4) {
                    document.getElementById('shipStatus').innerHTML = " שרות המשלוחים שלנו סגור עקב עננים שבורים בשמיים";
                }
                else {
                    document.getElementById('shipStatus').innerHTML = "שרות המשלוחים פעיל כעת";
                }
            };

        </script>
        <div id="shipStatus">
        </div>
